<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.user.mapper.TestMapper">
    <insert id="add" parameterType="User">
        insert into user1 values(#{id},#{name},#{username},#{password},#{desci})
    </insert>

    <select id="listTest" resultType="User">
        select * from user1
    </select>
</mapper>
